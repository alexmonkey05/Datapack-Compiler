def tellraw(var text) {
    /$tellraw @a $(text)
}

def title(var text) {
    /$title @a title $(text)
}

def remove_data(var type, var target, var path) {
    /$data remove $(type) $(target) $(path)
}

def playsound(var sound, var volume, var pitch) {
    execute (as @a at @s) /$playsound $(sound) weather @s ~ ~ ~ $(volume) $(pitch)
}

var scene_step = 0
var scene_delay = 0

var is_tutorial = false
var is_game = false

var in_text = false
var result_text = ""
var temp_text = ""

var left_time = 0
var max_time = 10

var burgers = [
    [], [], [], [], [], [], [], []
]

var smokers = [
    0, 0, 0, 0
]

def set_step(var step) {
    if (scene_delay <= 0 and scene_step == step) {
        return true
    } else {
        return false
    }
}

def load() {
    /clear @a
    /kill @e[type=!player]
    execute (as @a at @s) {
        /attribute @s block_break_speed base set 0
    }

    scene_step = 0
    scene_delay = 0

    is_tutorial = false
    is_game = false

    in_text = false
    result_text = ""
    temp_text = ""

    left_time = 0
    max_time = 10

    burgers = [
        [], [], [], [], [], [], [], []
    ]

    smokers = [
        0, 0, 0, 0
    ]

    /bossbar remove title 
    /bossbar remove text 

    /bossbar add title ""
    /bossbar add text ""

    /bossbar set title players @a
    /bossbar set text players @a

    /bossbar set title max 1000
    /bossbar set text max 1000

