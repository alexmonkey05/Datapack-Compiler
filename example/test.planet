def tick_tutorial() {
    if (set_step(1)) {

    }
}

def tick() {
    if (is_tutorial) tick_tutorial()
    
    /effect give @a night_vision infinite 255 true
    /kill @e[type=item]

    set_data("storage", "var", "temp_text", temp_text)
    if (in_text) {
        if (result_text != temp_text) { # 여기서 오류나네요
            /$data modify storage var:result_text result set string storage var:temp_text string 0 $(len(result_text) + 2)
        } else {
            in_text = false
            temp_text = ""
        }
    }
    result_text = get_data("storage", "var", "result_text")

    if (is_tutorial) {
        /bossbar set title name {"text":"[튜토리얼]","color":"green","bold":true}
        /$bossbar set text name {"text":"$(result_text)","color":"yellow"}

        /bossbar set minecraft:text color white
        /bossbar set text value 0
    } else if (is_game) {
        /bossbar set title name {"text":"[남은 시간]","color":"green","bold":true}
        /$bossbar set text name {"text":"$(left_time)","color":"yellow"}

        /bossbar set minecraft:text color green

        /$bossbar set text value $(round(divide(left_time,max_time) * 1000))
    } else {
        /bossbar set minecraft:text color white
        /bossbar set text value 0
    }
    
    execute (as @e[type=interaction]) remove_data("entity", "@s", "interaction")
}